{
  "React Functional Component": {
    "prefix": "rfc",
    "body": [
      "import { FC } from 'react';",
      "",
      "interface ${1:ComponentName}Props {",
      "  ${2}",
      "}",
      "",
      "export const ${1:ComponentName}: FC<${1:ComponentName}Props> = ({ ${3} }) => {",
      "  return (",
      "    <div>",
      "      ${4}",
      "    </div>",
      "  );",
      "};"
    ],
    "description": "Create a React Functional Component with TypeScript"
  },
  "React Hook": {
    "prefix": "rhook",
    "body": [
      "import { useState, useEffect } from 'react';",
      "",
      "export const use${1:HookName} = (${2}) => {",
      "  const [${3:state}, set${3/(.*)/${1:/capitalize}/}] = useState(${4});",
      "",
      "  useEffect(() => {",
      "    ${5}",
      "  }, [${6}]);",
      "",
      "  return { ${3:state} };",
      "};"
    ],
    "description": "Create a custom React Hook"
  },
  "API Route Handler": {
    "prefix": "api",
    "body": [
      "import { Request, Response } from 'express';",
      "",
      "export const ${1:handlerName} = async (req: Request, res: Response) => {",
      "  try {",
      "    ${2}",
      "    res.json({ success: true });",
      "  } catch (error) {",
      "    console.error('Error in ${1:handlerName}:', error);",
      "    res.status(500).json({ error: 'Internal server error' });",
      "  }",
      "};"
    ],
    "description": "Create an Express API route handler"
  },
  "Drizzle Schema": {
    "prefix": "dschema",
    "body": [
      "import { pgTable, text, timestamp, uuid } from 'drizzle-orm/pg-core';",
      "",
      "export const ${1:tableName} = pgTable('${1:tableName}', {",
      "  id: uuid('id').primaryKey().defaultRandom(),",
      "  ${2:columnName}: text('${2:columnName}').notNull(),",
      "  createdAt: timestamp('created_at').defaultNow().notNull(),",
      "  updatedAt: timestamp('updated_at').defaultNow().notNull(),",
      "});"
    ],
    "description": "Create a Drizzle ORM schema"
  }
}